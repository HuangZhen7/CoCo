/* pages/main/main.wxss */
swiper {
  height: 420rpx;
}
swiper-item image {
  width: 100%;
  height: 100%;
}
.main-order {
  padding-top: 50rpx;
}
.button-hover {
  background-color: white;
}
.main-btn {
  border-radius: 25rpx;
  border: 1px solid #e78424;
  width: 500rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 16px;
  color: #ec8319;
}
.main-items {
  padding-top: 20rpx;
}
.main-items .item{
  position: relative;
  display: flex;
  margin: 0 30rpx;
  padding: 30rpx 0;
  border-bottom:1px solid #e8e8e8;
  box-sizing: border-box;
}
.main-items .item:last-child {
  border: none;
}
.main-items .item image{
  height: 40rpx;
  width: 40rpx;
  margin-right: 20rpx;
}
.main-items .item text {
  color: #808080;
  font-size: 14px;
  align-items: center;
}
.main-items .item::after {
  position: absolute;
  content: '';
  right: 10rpx;
  /* float: right; */
  /* background-image: '/images/right.png'; */
  /* background: url('/images/right.png'); */
  background-image:url('data:image/png;base64,');

  background-size: contain;
  width: 40rpx;
  height: 40rpx;
  vertical-align: center;
}

.main-fd {
  height: 150rpx;
}
.main-fd image {
  height: 100%;
  width: 100%;
}