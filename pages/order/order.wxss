/* pages/order/order.wxss */
page{
  background-color: #f7f4f4;
}
.order {
  /* background-color: #d4d4d4; */
  margin: 40rpx;
}
.orderMsg{
  position: relative;
  border: 1px solid rgb(190, 185, 185);
  border-radius: 20rpx;
  height: 400rpx;
  width: 100%;
  background-color: #fff;
}
.orderMsg text {
  position: absolute;
  font-size: 30rpx;
  color: #b8b8b8;
  top: 50%;
  left: 50%;
  margin-left:-30px;
  margin-top:-10px;
}
.orderBtn {
  border-radius: 10rpx;
  border: 1px solid #ef7d0d;
  background-color: #ef7d0d;
  width: 100%;
  height: 40px;
  margin-top: 30px;
  color: #fff;
  font-size: 14px;
  line-height: 40px;


}